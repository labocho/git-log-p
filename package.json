{
	"name": "git-log-p",
	"displayName": "git log -p",
	"description": "Show `git log -p $CURRENT_FILE`",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.gitLogP"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.gitLogP",
			"title": "git log -p $CURRENT_FILE"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"publisher": "labocho",
	"repository": {
		"type": "git",
		"url": "https://github.com/labocho/git-log-p.git"
	}
}
